package ir;

import java.util.ArrayList;
import java.util.List;

public class Backtraking {
	
	
	public static void main(String [] args){
		ArrayList<String> list1 = new ArrayList<String>();
		ArrayList<String> list2 = new ArrayList<String>();
		ArrayList<String> list3 = new ArrayList<String>();
		List<List<String>> list = new ArrayList<List<String>>();
		
		list1.add("a1");
		list1.add("a2");
		
		list2.add("b1");
		list2.add("b2");
		list2.add("b3");
		
		list3.add("c1");
		list3.add("c2");
		list3.add("c3");
		
		
		list.add(list1);
		list.add(list2);
		list.add(list3);
		
		buildLevel(list);
	
	}
	
	public static void buildLevel(List<List<String>> list){
		int size = list.size();
		switch(size){
		case 2 : buildLevel2(list); break;
		case 3 : buildLevel3(list); break;
		case 4 : buildLevel4(list); break;
		case 5 : buildLevel5(list); break;
		case 6 : buildLevel6(list); break;
		case 7 : buildLevel7(list); break;
		
		}
	}
	
	public static void buildLevel2(List<List<String>> list){
		for(String w1 : list.get(0)){
			for(String w2 : list.get(1)){
				System.out.println(w1+" "+w2);
			}
			
		}
	}
	
	public static void buildLevel3(List<List<String>> list){
		for(String w1 : list.get(0)){
			for(String w2 : list.get(1)){
				for(String w3: list.get(2)){
					System.out.println(w1+" "+w2+" "+w3);
				}
				
			}
			
		}
	}
	
	public static void buildLevel4(List<List<String>> list){
		for(String w1 : list.get(0)){
			for(String w2 : list.get(1)){
				for(String w3: list.get(2)){
					for(String w4: list.get(3)){
						System.out.println(w1+" "+w2+" "+w3+" "+w4);
					}
					
				}
				
			}
			
		}
	}
	
	public static void buildLevel5(List<List<String>> list){
		for(String w1 : list.get(0)){
			for(String w2 : list.get(1)){
				for(String w3: list.get(2)){
					for(String w4: list.get(3)){
						for(String w5: list.get(4)){
							System.out.println(w1+" "+w2+" "+w3+" "+w4+" "+w5);
						}
						
					}
					
				}
				
			}
			
		}
	}
	
	public static void buildLevel6(List<List<String>> list){
		for(String w1 : list.get(0)){
			for(String w2 : list.get(1)){
				for(String w3: list.get(2)){
					for(String w4: list.get(3)){
						for(String w5: list.get(4)){
							for(String w6: list.get(5)){
								System.out.println(w1+" "+w2+" "+w3+" "+w4+" "+w5+" "+w6);
							}
							
						}
						
					}
					
				}
				
			}
			
		}
	}
	
	public static void buildLevel7(List<List<String>> list){
		for(String w1 : list.get(0)){
			for(String w2 : list.get(1)){
				for(String w3: list.get(2)){
					for(String w4: list.get(3)){
						for(String w5: list.get(4)){
							for(String w6: list.get(5)){
								for(String w7: list.get(6)){
									System.out.println(w1+" "+w2+" "+w3+" "+w4+" "+w5+" "+w6+" "+w7);
								}
								
							}
							
						}
						
					}
					
				}
				
			}
			
		}
	}



}
